@model IEnumerable<JabilEjercicio.Models.Detail>

@{
    ViewData["Title"] = "Index";
}

<h1>Details :p</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Building)
            </th>
            <th> @Html.DisplayNameFor(model => model.Customer)</th>
            <th> @Html.DisplayNameFor(model => model.PartNumber)</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Building)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Customer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PartNumber)
            </td>
        </tr>
}
    </tbody>
</table>
